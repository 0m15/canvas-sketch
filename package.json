{
  "name": "canvas-sketch",
  "version": "0.0.4",
  "description": "A utility for quickly prototyping 2D and WebGL sketches",
  "main": "./dist/canvas-sketch.umd.js",
  "module": "./dist/canvas-sketch.m.js",
  "license": "MIT",
  "author": {
    "name": "Matt DesLauriers",
    "email": "dave.des@gmail.com",
    "url": "https://github.com/mattdesl"
  },
  "dependencies": {
    "convert-units": "^2.3.4",
    "dateformat": "^3.0.3",
    "deep-equal": "^1.0.1",
    "defined": "^1.0.0",
    "factor-bundle": "^2.5.0",
    "get-canvas-context": "^1.0.2",
    "is-class": "0.0.5",
    "is-dom": "^1.0.9",
    "is-promise": "^2.1.0",
    "object-assign": "^4.1.1",
    "pad-left": "^2.1.0",
    "right-now": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.51",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.51",
    "@babel/plugin-syntax-jsx": "^7.0.0-beta.51",
    "@babel/plugin-transform-react-jsx": "^7.0.0-beta.51",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.51",
    "@babel/runtime": "^7.0.0-beta.51",
    "@mattdesl/microbundle": "^0.4.4",
    "array-shuffle": "^1.0.1",
    "babelify": "^9.0.0",
    "browserify": "^16.2.2",
    "budo": "^11.3.0",
    "concat-stream": "^1.6.2",
    "duplexer2": "^0.1.4",
    "eases": "^1.0.8",
    "glsl-aastep": "^1.0.1",
    "glsl-dither": "^1.0.1",
    "glsl-hsl2rgb": "^1.1.0",
    "glsl-noise": "0.0.0",
    "glslify": "^6.1.1",
    "hex-rgb": "^3.0.0",
    "history": "^4.7.2",
    "lerp": "^1.0.3",
    "load-asset": "^1.2.0",
    "microbundle": "^0.4.4",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.3",
    "perspective-camera": "^2.0.1",
    "preact": "^8.2.9",
    "preact-router": "^2.6.1",
    "primitive-icosphere": "^1.0.2",
    "primitive-quad": "^2.0.0",
    "regl": "^1.3.6",
    "rimraf": "^2.6.2",
    "seed-random": "^2.2.0",
    "surge": "^0.20.1",
    "three": "^0.93.0",
    "through2": "^2.0.3",
    "uglify-es": "^3.3.9",
    "watchify": "^3.11.0"
  },
  "scripts": {
    "prepublishOnly": "npm run lib:bundle",
    "lib:bundle": "microbundle --entry lib/index.js --target=browser --sourcemap=inline --no-compress --format=es,umd -o ./dist/",
    "lib:watch": "microbundle watch --entry lib/index.js --target=browser --sourcemap=inline --no-compress --format=es,umd -o ./dist/",
    "lib:server": "budo examples/index.js:bundle.js --dir examples --live -- -t glslify",
    "lib": "npm-run-all --parallel lib:watch lib:server",
    "start": "npm-run-all --parallel lib:watch site:dev",
    "site:dev": "node website/dev.js",
    "site:bundle": "NODE_ENV=production node website/dev.js",
    "build": "mkdir -p dist/ && npm run lib:bundle && site:bundle"
  },
  "keywords": [
    "sketch",
    "2d",
    "processing",
    "js",
    "p5js",
    "3d",
    "webgl",
    "gl",
    "opengl",
    "threejs",
    "canvas",
    "canvas2d",
    "generative",
    "art",
    "generativeart"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/mattdesl/canvas-sketch.git"
  },
  "homepage": "https://github.com/mattdesl/canvas-sketch",
  "bugs": {
    "url": "https://github.com/mattdesl/canvas-sketch/issues"
  }
}
